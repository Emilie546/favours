<div class="container">
  <div class="intro">
    <h3>My favours</h3>
  </div>
  <div class="status">
    <% @contracts.each do |c| %>
      <%= link_to favour_path(c) do %>
        <div class="card">
          <div class="card-body">
            <p class="card-text">Name of the favour: <%= c.name %></p>
            <p class="card-text">Start time : <%= c.start_time %></p>
            <p class="card-text">End time : <%= c.end_time %></p>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
